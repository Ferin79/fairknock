          CITY OF
          <span style="text-decoration: underline"
            ><%= userAnswerTemplate.property.city %></span
          >
          , COUNTY OF
          <span style="text-decoration: underline">INSERT_COUNTY_NAME</span> ,
          STATE OF CALIFORNIA, DESCRIBED AS
          <span style="text-decoration: underline"
            ><%= userAnswerTemplate.property.addressLine1 %> <%=
            userAnswerTemplate.property.addressLine2 %> <%=
            userAnswerTemplate.property.city %> <%=
            userAnswerTemplate.property.zipCode %> <%=
            userAnswerTemplate.property.state.name %></span
